<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@color/colorPrimary"
    >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="17sp"
            android:layout_marginRight="20sp">
            <TextView
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:background="@color/colorPrimary"
                android:textStyle="bold"
                android:text="Account"
                android:textSize="27dp"
                android:textColor="#ffffff"/>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="0dp"
                android:src="@drawable/search"/>

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="59dp"
                android:src="@drawable/list" />

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="40sp">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="17sp"
            android:layout_marginRight="20sp">
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/home"
                android:layout_marginEnd="40sp">
            </ImageView>

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/fuego"
                android:layout_marginEnd="40sp"/>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/suscriptor"
                android:layout_marginEnd="60sp"/>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/persona"
                android:layout_marginRight="34dp"/>

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="17sp"
            android:layout_marginTop="90sp"
            android:background="@color/colorAccent"
            android:layout_marginRight="20sp">
            <ImageView
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:src="@drawable/cantante"
                android:layout_gravity="center"
                android:layout_marginLeft="34sp"/>

            <View
                android:layout_width="32sp"
                android:layout_height="70sp"
                android:background="@color/colorAccent">
            </View>

            <LinearLayout
                android:layout_width="69dp"
                android:layout_height="69dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="7sp"
                    android:layout_marginRight="20sp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingLeft="12sp"
                        android:paddingRight="12sp"
                        android:text="Account"
                        android:textColor="@color/colorAccent"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <View
                        android:layout_width="140dp"
                        android:layout_height="25dp">

                    </View>

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:src="@drawable/search" />

                    <View
                        android:layout_width="15dp"
                        android:layout_height="25dp">

                    </View>

                    <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="36dp"
                        android:layout_height="30dp"
                        android:gravity="right"
                        android:src="@drawable/list" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <TextView
        android:layout_width="130dp"
        android:layout_height="30sp"
        android:text="Mary Smith"
        android:textSize="12dp"
        android:textColor="@color/colorPrimaryDark"
        android:layout_marginTop="150dp"
        android:gravity="center"
        android:layout_marginLeft="17sp"
        android:background="@color/colorAccent"/>
    <ImageView
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:src="@drawable/icono"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="140dp">

    </ImageView>
    <View
        android:layout_width="108dp"
        android:layout_height="30dp"
        android:layout_marginTop="150dp"
        android:background="@color/colorAccent"
        android:foregroundGravity="right"
        android:layout_marginLeft="169sp"
        android:id="@+id/view">

    </View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:orientation="horizontal"
        android:layout_marginTop="180dp"
        android:background="#ffffff"
        android:layout_marginLeft="17dp"
        android:layout_alignRight="@+id/view"
        android:layout_alignEnd="@+id/view"
        android:id="@+id/linearLayout3">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:background="@drawable/history"
                android:layout_marginLeft="17dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="     History"
                android:textSize="20dp"
                android:textStyle="bold"
                android:textColor="@color/colorPrimaryDark"
                android:gravity="center"/>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/camara"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="5dp"
                android:layout_gravity="right"
                />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:orientation="horizontal"
        android:layout_marginTop="220dp"
        android:background="#ffffff"
        android:layout_marginLeft="17dp"
        android:layout_alignRight="@+id/linearLayout3"
        android:layout_alignEnd="@+id/linearLayout3"
        android:id="@+id/linearLayout4">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:src="@drawable/libreria"
                android:layout_marginTop="18dp"
                android:layout_marginLeft="17dp"
                android:layout_gravity="top"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="     My videos"
                android:textSize="20dp"
                android:textStyle="bold"
                android:textColor="@color/colorPrimaryDark"
                android:gravity="center"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:orientation="horizontal"
        android:layout_marginTop="270dp"
        android:background="#ffffff"
        android:layout_marginLeft="17dp"
        android:id="@+id/linearLayout"
        android:layout_alignRight="@+id/linearLayout4"
        android:layout_alignEnd="@+id/linearLayout4">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:src="@drawable/alarma"
                android:layout_marginTop="18dp"
                android:layout_marginLeft="17dp"
                android:layout_gravity="top"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="     Watch Later"
                android:textSize="20dp"
                android:textStyle="bold"
                android:textColor="@color/colorPrimaryDark"
                android:gravity="center"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:orientation="horizontal"
        android:background="#ffffff"
        android:layout_below="@+id/linearLayout"
        android:layout_alignLeft="@+id/linearLayout2"
        android:layout_alignStart="@+id/linearLayout2"
        android:layout_alignRight="@+id/linearLayout"
        android:layout_alignEnd="@+id/linearLayout">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:src="@drawable/video"
                android:layout_marginTop="18dp"
                android:layout_marginLeft="17dp"
                android:layout_gravity="top"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="     Get YouTube Red"
                android:textSize="20dp"
                android:textStyle="bold"
                android:textColor="@color/colorPrimaryDark"
                android:gravity="center"/>
        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:orientation="horizontal"
        android:layout_marginTop="378dp"
        android:background="#ffffff"
        android:layout_marginLeft="17dp"
        android:id="@+id/linearLayout2"
        android:layout_alignRight="@+id/linearLayout"
        android:layout_alignEnd="@+id/linearLayout">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <View
                android:layout_width="261dp"
                android:layout_height="wrap_content"
                android:background="#ffffff">
            </View>

        </LinearLayout>

    </LinearLayout>

</RelativeLayout>





